            android:id="@+id/action_autoImageSliderFragment_to_dashboardFragment"
            app:destination="@id/dashboardFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"/>
    </fragment>
    <fragment
        android:id="@+id/whatsAppStoryViewFragment"
        android:name="com.development.mydaggerhiltmvvm.view.fragment.dashboard_fragment.WhatsAppStoryViewFragment"
        android:label="WhatsApp StoryView" >
        <action
            android:id="@+id/action_whatsAppStoryViewFragment_to_dashboardFragment"
            app:destination="@id/dashboardFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"/>
    </fragment>
    <fragment
        android:id="@+id/instagramSuggestionViewpage2Fragment"
        android:name="com.development.mydaggerhiltmvvm.view.fragment.dashboard_fragment.InstagramSuggestionViewpage2Fragment"
        android:label="Instagram Suggestion" />
    <fragment
        android:id="@+id/chatFragment"
        android:name="com.development.mydaggerhiltmvvm.view.fragment.dashboard_fragment.ChatFragment"
        android:label="ChatFragment" />
</navigation>
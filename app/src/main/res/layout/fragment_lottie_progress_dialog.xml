            android:layout_marginTop="@dimen/dimens_10dp"
            android:text="Example 8" />

        <com.google.android.material.button.MaterialButton
            android:id="@+id/bt9"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toBottomOf="@+id/bt8"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginStart="@dimen/dimens_20dp"
            android:layout_marginEnd="@dimen/dimens_20dp"
            android:layout_marginTop="@dimen/dimens_10dp"
            android:text="Example 9" />

        <com.google.android.material.button.MaterialButton
            android:id="@+id/bt10"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toBottomOf="@+id/bt9"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginStart="@dimen/dimens_20dp"
            android:layout_marginEnd="@dimen/dimens_20dp"
            android:layout_marginTop="@dimen/dimens_10dp"
            android:text="Example 10" />

    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>
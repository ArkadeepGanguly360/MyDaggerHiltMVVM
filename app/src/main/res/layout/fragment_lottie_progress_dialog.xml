            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginStart="@dimen/dimens_20dp"
            android:layout_marginEnd="@dimen/dimens_20dp"
            android:layout_marginTop="@dimen/dimens_10dp"
            android:text="Example 5" />

        <com.google.android.material.button.MaterialButton
            android:id="@+id/bt6"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toBottomOf="@+id/bt5"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginStart="@dimen/dimens_20dp"
            android:layout_marginEnd="@dimen/dimens_20dp"
            android:layout_marginTop="@dimen/dimens_10dp"
            android:text="Example 6" />

        <com.google.android.material.button.MaterialButton
            android:id="@+id/bt7"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toBottomOf="@+id/bt6"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginStart="@dimen/dimens_20dp"
            android:layout_marginEnd="@dimen/dimens_20dp"
            android:layout_marginTop="@dimen/dimens_10dp"
            android:text="Example 7" />

        <com.google.android.material.button.MaterialButton
            android:id="@+id/bt8"
            android:layout_width="0dp"

            android:layout_marginStart="@dimen/dimens_20dp"
            android:layout_marginEnd="@dimen/dimens_20dp"
            android:layout_marginTop="@dimen/dimens_10dp"
            android:text="Example 3" />

        <com.google.android.material.button.MaterialButton
            android:id="@+id/bt4"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toBottomOf="@+id/bt3"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginStart="@dimen/dimens_20dp"
            android:layout_marginEnd="@dimen/dimens_20dp"
            android:layout_marginTop="@dimen/dimens_10dp"
            android:text="Example 4" />

        <com.google.android.material.button.MaterialButton
            android:id="@+id/bt5"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toBottomOf="@+id/bt4"
